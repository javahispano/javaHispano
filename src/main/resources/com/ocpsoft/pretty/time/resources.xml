<properties>

    <!-- Century -->
	<entry key="CenturyPattern">%n %u</entry>
	<entry key="CenturyFuturePrefix"></entry>
	<entry key="CenturyFutureSuffix"> from now</entry>
	<entry key="CenturyPastPrefix"></entry>
	<entry key="CenturyPastSuffix"> ago</entry>
	<entry key="CenturyName">century</entry>
	<entry key="CenturyPluralName">centuries</entry>

    <!-- Day -->
	<entry key="DayPattern">%n %u</entry>
	<entry key="DayFuturePrefix"></entry>
	<entry key="DayFutureSuffix"> from now</entry>
	<entry key="DayPastPrefix"></entry>
	<entry key="DayPastSuffix"> ago</entry>
	<entry key="DayName">day</entry>
	<entry key="DayPluralName">days</entry>

    <!-- Decade -->
	<entry key="DecadePattern">%n %u</entry>
	<entry key="DecadeFuturePrefix"></entry>
	<entry key="DecadeFutureSuffix"> from now</entry>
	<entry key="DecadePastPrefix"></entry>
	<entry key="DecadePastSuffix"> ago</entry>
	<entry key="DecadeName">decade</entry>
	<entry key="DecadePluralName">decades</entry>

