<properties>

    <!-- Century -->
	<entry key="CenturyPattern">%n %u</entry>
	<entry key="CenturyFuturePrefix">dans </entry>
	<entry key="CenturyFutureSuffix"></entry>
	<entry key="CenturyPastPrefix">il y a </entry>
	<entry key="CenturyPastSuffix"></entry>
	<entry key="CenturyName">siècle</entry>
	<entry key="CenturyPluralName">siècles</entry>

    <!-- Day -->
	<entry key="DayPattern">%n %u</entry>
	<entry key="DayFuturePrefix">dans </entry>
	<entry key="DayFutureSuffix"></entry>
	<entry key="DayPastPrefix">il y a </entry>
	<entry key="DayPastSuffix"></entry>
	<entry key="DayName">jour</entry>
	<entry key="DayPluralName">jours</entry>

    <!-- Decade -->
