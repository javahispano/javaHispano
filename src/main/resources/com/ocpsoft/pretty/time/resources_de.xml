<properties>

    <!-- Century -->
	<entry key="CenturyPattern">%n %u</entry>
	<entry key="CenturyFuturePrefix">In </entry>
	<entry key="CenturyFutureSuffix"></entry>
	<entry key="CenturyPastPrefix">Vor </entry>
	<entry key="CenturyPastSuffix"></entry>
	<entry key="CenturyName">Jahrhundert</entry>
	<entry key="CenturyPluralName">Jahrhunderten</entry>

    <!-- Day -->
	<entry key="DayPattern">%n %u</entry>
	<entry key="DayFuturePrefix">In </entry>
	<entry key="DayFutureSuffix"></entry>
	<entry key="DayPastPrefix">Vor </entry>
	<entry key="DayPastSuffix"></entry>
	<entry key="DayName">Tag</entry>
	<entry key="DayPluralName">Tagen</entry>

    <!-- Decade -->
	<entry key="DecadePattern">%n %u</entry>
	<entry key="DecadeFuturePrefix">In </entry>
	<entry key="DecadeFutureSuffix"></entry>
	<entry key="DecadePastPrefix">Vor </entry>
	<entry key="DecadePastSuffix"></entry>
	<entry key="DecadeName">Jahrzehnt</entry>
	<entry key="DecadePluralName">Jahrzehnten</entry>

    <!-- Hour -->
	<entry key="HourPattern">%n %u</entry>
	<entry key="HourFuturePrefix">In </entry>
	<entry key="HourFutureSuffix"></entry>
	<entry key="HourPastPrefix">Vor </entry>
	<entry key="HourPastSuffix"></entry>
	<entry key="HourName">Stunde</entry>
	<entry key="HourPluralName">Stunden</entry>

    <!-- JustNow -->
	<entry key="JustNowPattern">%u</entry>
	<entry key="JustNowFuturePrefix">Jetzt</entry>
	<entry key="JustNowFutureSuffix"></entry>
	<entry key="JustNowPastPrefix">Vor einem Augenblick</entry>
	<entry key="JustNowPastSuffix"></entry>
	<entry key="JustNowName"></entry>
	<entry key="JustNowPluralName"></entry>

    <!-- Millennium -->
	<entry key="MillenniumPattern">%n %u</entry>
	<entry key="MillenniumFuturePrefix">In </entry>
	<entry key="MillenniumFutureSuffix"></entry>
	<entry key="MillenniumPastPrefix">Vor </entry>
	<entry key="MillenniumPastSuffix"></entry>
	<entry key="MillenniumName">Jahrtausend</entry>
	<entry key="MillenniumPluralName">Jahrtausenden</entry>

    <!-- Millisecond -->
	<entry key="MillisecondPattern">%n %u</entry>
	<entry key="MillisecondFuturePrefix">In </entry>
	<entry key="MillisecondFutureSuffix"></entry>
	<entry key="MillisecondPastPrefix">Vor </entry>
	<entry key="MillisecondPastSuffix"></entry>
	<entry key="MillisecondName">Millisekunde</entry>
	<entry key="MillisecondPluralName">Millisekunden</entry>

    <!-- Minute -->
	<entry key="MinutePattern">%n %u</entry>
	<entry key="MinuteFuturePrefix">In </entry>
	<entry key="MinuteFutureSuffix"></entry>
	<entry key="MinutePastPrefix">Vor </entry>
	<entry key="MinutePastSuffix"></entry>
	<entry key="MinuteName">Minute</entry>
	<entry key="MinutePluralName">Minuten</entry>

    <!-- Month -->
	<entry key="MonthPattern">%n %u</entry>
	<entry key="MonthFuturePrefix">In </entry>
	<entry key="MonthFutureSuffix"></entry>
	<entry key="MonthPastPrefix">Vor </entry>
	<entry key="MonthPastSuffix"></entry>
	<entry key="MonthName">Monat</entry>
	<entry key="MonthPluralName">Monaten</entry>

    <!-- Second -->
	<entry key="SecondPattern">%n %u</entry>
	<entry key="SecondFuturePrefix">In </entry>
	<entry key="SecondFutureSuffix"></entry>
	<entry key="SecondPastPrefix">Vor </entry>
	<entry key="SecondPastSuffix"></entry>
	<entry key="SecondName">Sekunde</entry>
	<entry key="SecondPluralName">Sekunden</entry>

    <!-- Week -->
	<entry key="WeekPattern">%n %u</entry>
	<entry key="WeekFuturePrefix">In </entry>
	<entry key="WeekFutureSuffix"></entry>
	<entry key="WeekPastPrefix">Vor </entry>
	<entry key="WeekPastSuffix"></entry>
	<entry key="WeekName">Woche</entry>
	<entry key="WeekPluralName">Wochen</entry>

    <!-- Year -->
	<entry key="YearPattern">%n %u</entry>
	<entry key="YearFuturePrefix">In </entry>
	<entry key="YearFutureSuffix"></entry>
	<entry key="YearPastPrefix">Vor </entry>
	<entry key="YearPastSuffix"></entry>
	<entry key="YearName">Jahr</entry>
	<entry key="YearPluralName">Jahren</entry>

    <!-- AbstractTimeUnit -->
	<entry key="AbstractTimeUnitPattern"></entry>
	<entry key="AbstractTimeUnitFuturePrefix"></entry>
	<entry key="AbstractTimeUnitFutureSuffix"></entry>
	<entry key="AbstractTimeUnitPastPrefix"></entry>
	<entry key="AbstractTimeUnitPastSuffix"></entry>
	<entry key="AbstractTimeUnitName"></entry>
	<entry key="AbstractTimeUnitPluralName"></entry>

</properties>